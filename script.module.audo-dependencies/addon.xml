    <import addon="os.openelec.tv" version="3.0"/>
    <import addon="xbmc.addon" version="12.0.0"/>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
